package com.neotech.lesson32;

import java.util.Hashtable;

public class HashTable {

	public static void main(String[] args) {
		
		
		Hashtable <Integer, String > ht = new Hashtable();
		
		
		ht.put(2, "Mustafa");
		ht.put(1, "Gloria");
		ht.put(3, "Mehmet");
		ht.put(4, "Serkan");
		
		System.out.println(ht);
		
		
		
		

	}

}
